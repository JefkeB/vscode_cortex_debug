{
    "configurations": [
        {
            "name": "gcc",
            "includePath": [
                "${workspaceFolder}/Inc",
                "${workspaceFolder}/Common/Drivers/**",
                "${workspaceFolder}/Common/Middlewares/**"
            ],
            "defines": [
                "DEBUG",
                "USE_HAL_DRIVER",
                "STM32F407xx"
            ],
            "compilerPath": "${env:GNUARM}",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-x64",
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "${env:GNUARM}"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            }
        }
    ],
    "version": 4
}